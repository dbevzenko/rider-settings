<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Build Selected Projects">
        <Item>
          <IdeaActionID>BuildSelection</IdeaActionID>
          <ShortCut>'⌘`, B'</ShortCut>
          <Description>Build Selected Projects</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Build Solution">
        <Item>
          <IdeaActionID>BuildSelection</IdeaActionID>
          <ShortCut>'⌘`, B'</ShortCut>
          <Description>Build Solution</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Emoji &amp; Symbols">
        <Item>
          <IdeaActionID>EmojiAndSymbols</IdeaActionID>
          <ShortCut>'⌃⌘Space'</ShortCut>
          <Description>Emoji &amp; Symbols</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameRiderItem</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'EOG.OmniSci.Client.T...'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧R'</ShortCut>
          <Description>Run 'EOG.OmniSci.Client.T...'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run Current Session">
        <Item>
          <IdeaActionID>RiderUnitTestRunCurrentSessionAction</IdeaActionID>
          <ShortCut>'⌘;, Y' or '⌘;, ⌘Y'</ShortCut>
          <Description>Run Current Session</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Unit Testing Quick List...">
        <Item>
          <IdeaActionID>RiderUnitTestQuickListPopupAction</IdeaActionID>
          <ShortCut>'⌥⇧U'</ShortCut>
          <Description>Unit Testing Quick List...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Unit Tests">
        <Item>
          <IdeaActionID>ActivateUnitTestsToolWindow</IdeaActionID>
          <ShortCut>'⌘8' or '⇧⌘8'</ShortCut>
          <Description>Unit Tests</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>Declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>